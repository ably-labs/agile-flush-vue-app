{
  "name": "agile-flush",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "init": "npm clean-install && cd api && npm clean-install",
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "func": "cd api && func start",
    "swa": "swa start http://localhost:5173 --api-location http://localhost:7071",
    "all": "npm-run-all --parallel dev func swa",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore --fix src"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ably-labs/agile-flush-vue-app"
  },
  "author": "Marc Duiker",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/ably-labs/agile-flush-vue-app/issues"
  },
  "dependencies": {
    "ably": "^1.2.15",
    "vue": "^3.2.26",
    "vue-router": "^4.0.12",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@azure/static-web-apps-cli": "^1.0.3",
    "@vitejs/plugin-vue": "^3.2.0",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-vue": "^8.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "vite": "^3.2.7"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
